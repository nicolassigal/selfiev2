{
  "name": "selfiev2",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:prod",
    "build:prod": "ng build --configuration=production --aot --base-href /selfie-v2/ --build-optimizer --optimization",
    "build:beta": "ng build --configuration=beta --aot --base-href /selfie-v2/ --build-optimizer --optimization",
    "firebase": "firebase deploy && firebase open hosting:site",
    "git": "npm run git:add && npm run git:commit && npm run  git:push",
    "git:pull": "git pull origin master",
    "git:add": "git add .",
    "git:commit": "git commit -m 'asdas'",
    "git:push": "git push -u origin master",
    "deploy": "npm run deploy:prod",
    "deploy:prod": "npm run build && npm run git && nrpm run ftp",
    "deploy:beta": "npm run build:beta && npm run ftp:beta",
    "ftp": "npm run ftp:prod",
    "ftp:prod": "NODE_ENV=production node deploy-ftp.js",
    "ftp:beta": "NODE_ENV=beta node deploy-ftp.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
